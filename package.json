        "eslint-plugin-jest-dom": "^4.0.3",
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.0",
        "gulp-flatten": "^0.4.0",
        "gulp-rename": "^2.0.0",
        "gulp-uglify": "^3.0.2",
